{
  "name": "vrchat",
  "description": "Unofficial VRChat API",
  "version": "0.0.5",
  "author": "Marei Kikukawa <contact@calmery.me>",
  "bugs": "https://github.com/calmery/VRChatJS/issues",
  "dependencies": {
    "axios": "0.19.0",
    "cookie": "0.4.0"
  },
  "devDependencies": {
    "@types/cookie": "0.3.3",
    "@types/jest": "25.2.2",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.5",
    "jest": "26.0.1",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "ts-jest": "25.5.1",
    "typescript": "3.6.3"
  },
  "engine": {
    "node": "10.16.3",
    "npm": "6.11.3"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/calmery/VRChatJS",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "roots": [
      "<rootDir>/tests"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "keywords": [
    "vrchat"
  ],
  "license": "MIT",
  "lint-staged": {
    "**/*.ts": [
      "npm run prettify",
      "git add"
    ]
  },
  "main": "build",
  "repository": {
    "type": "git",
    "url": "https://github.com/calmery/VRChatJS.git"
  },
  "scripts": {
    "build": "rimraf build && tsc",
    "prepare": "tsc",
    "prettify": "eslint --fix src/ tests/ --ext .ts",
    "test": "jest --coverage",
    "watch": "rimraf build && tsc --watch"
  }
}
