{
  "name": "vrchat",
  "description": "Unofficial VRChat API",
  "version": "0.0.3",
  "author": "Marei Kikukawa <contact@marei.jp>",
  "bugs": "https://github.com/calmery/VRChatJS/issues",
  "dependencies": {
    "axios": "0.18.1",
    "cookie": "0.3.1"
  },
  "devDependencies": {
    "@types/cookie": "0.3.2",
    "@types/jest": "24.0.15",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "2.4.1",
    "jest": "24.8.0",
    "lint-staged": "8.2.1",
    "prettier": "1.17.0",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "typescript": "3.5.2"
  },
  "engine": {
    "node": "10.16.3",
    "npm": "6.11.3"
  },
  "files": [
    "build"
  ],
  "homepage": "https://github.com/calmery/VRChatJS",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "roots": [
      "<rootDir>/tests"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "keywords": [
    "vrchat"
  ],
  "license": "MIT",
  "lint-staged": {
    "**/*.ts": [
      "npm run prettify",
      "git add"
    ]
  },
  "main": "build",
  "repository": {
    "type": "git",
    "url": "https://github.com/calmery/VRChatJS.git"
  },
  "scripts": {
    "build": "rimraf build && tsc",
    "prepare": "tsc",
    "prettify": "eslint --fix src/ tests/ --ext .ts",
    "test": "jest --coverage",
    "watch": "rimraf build && tsc --watch"
  }
}
